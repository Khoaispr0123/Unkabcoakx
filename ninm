function EquipTool(ToolSe)
	if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
		local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
		wait(0.5)
		game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
	end
end

local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

local ActiveTween -- Lưu Tween đang chạy

function Tween(P1)
    if not P1 then return end

    local Distance = (P1.Position - HRP.Position).Magnitude
    local Speed = 300 -- Tốc độ di chuyển trên không
    local HoverHeight = 50 -- Độ cao bay trên mặt đất

    if Distance < 1 then return end -- Nếu đã gần thì không Tween nữa

    -- Hủy Tween trước đó (nếu có)
    if ActiveTween then
        ActiveTween:Cancel()
    end

    -- **BƯỚC 1: Bay lên cao trước khi di chuyển**
    local UpCFrame = HRP.CFrame * CFrame.new(0, HoverHeight, 0)
    ActiveTween = TweenService:Create(HRP, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = UpCFrame})
    ActiveTween:Play()
    ActiveTween.Completed:Wait()

    -- **BƯỚC 2: Bay đến vị trí đích nhưng vẫn giữ độ cao**
    local MidCFrame = CFrame.new(P1.Position.X, HRP.Position.Y, P1.Position.Z)
    ActiveTween = TweenService:Create(HRP, TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear), {CFrame = MidCFrame})
    ActiveTween:Play()
    ActiveTween.Completed:Wait()

    -- **BƯỚC 3: Hạ xuống từ từ khi đến nơi**
    ActiveTween = TweenService:Create(HRP, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {CFrame = P1})
    ActiveTween:Play()
    ActiveTween.Completed:Wait()
    ActiveTween:Cancel()
end

function Tween2(P1)
    if not P1 then return end

    local Distance = (P1.Position - HRP.Position).Magnitude
    local Speed = 100

    if Distance < 1 then return end

    -- Hủy Tween cũ nếu đang chạy
    if ActiveTween then
        ActiveTween:Cancel()
    end

    -- **Di chuyển thẳng tới vị trí**
    ActiveTween = TweenService:Create(HRP, TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear), {CFrame = P1})
    ActiveTween:Play()
    ActiveTween.Completed:Wait()
end

function CancelTween(target)
    if not target then
        _G.StopTween = true
        wait()
        Tween(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
        wait()
        _G.StopTween = false
    end
end


